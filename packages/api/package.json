{
  "name": "@acme/api",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./dist/src/*.d.ts",
      "default": [
        "./src/*.ts",
        "./src/*.tsx"
      ]
    },
    "./**/*": {
      "types": "./dist/src/**/*.d.ts",
      "default": [
        "./src/**/*.ts",
        "./src/**/*.tsx"
      ]
    }
  },
  "scripts": {
    "build": "tsc",
    "generate:types": "openapi-typescript http://localhost:1337/documentation/v1/openapi.json --output src/types/strapi.d.ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "catalog:api",
    "@strapi/strapi": "5.1.0",
    "axios": "catalog:api",
    "graphql": "^16.9.0",
    "graphql-tag": "^2.12.6",
    "openapi-fetch": "^0.13.3",
    "openapi-typescript": "catalog:api",
    "qs": "^6.13.1"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:*",
    "@acme/tsconfig": "workspace:*",
    "@types/qs": "^6.9.17"
  }
}
