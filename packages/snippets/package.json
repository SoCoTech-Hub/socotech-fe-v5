{
  "name": "@acme/snippets",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./dist/src/*.d.ts",
      "default": [
        "./src/*.ts",
        "./src/*.tsx"
      ]
    },
    "./**/*": {
      "types": "./dist/src/**/*.d.ts",
      "default": [
        "./src/**/*.ts",
        "./src/**/*.tsx"
      ]
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "test": "jest",
    "gen-types": "npx graphql-codegen"
  },
  "devDependencies": {
    "@acme/api": "workspace:*",
    "@acme/eslint-config": "workspace:*",
    "@acme/prettier-config": "workspace:*",
    "@acme/tsconfig": "workspace:*",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/typescript": "^4.1.1",
    "@graphql-codegen/typescript-operations": "^4.3.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.2",
    "@testing-library/react": "catalog:jest",
    "@testing-library/user-event": "catalog:jest",
    "@types/jest": "catalog:jest",
    "@types/node": "^20",
    "@types/react": "catalog:react19",
    "@types/web-push": "^3.6.4",
    "eslint": "catalog:",
    "jest": "catalog:jest",
    "jest-environment-jsdom": "catalog:jest",
    "prettier": "catalog:",
    "react": "catalog:react19",
    "ts-jest": "catalog:jest",
    "typescript": "catalog:",
    "zod": "catalog:"
  },
  "peerDependencies": {
    "react": "catalog:react19",
    "zod": "catalog:"
  },
  "dependencies": {
    "@apollo/client": "catalog:api",
    "@tanstack/react-query": "^5.62.7",
    "cookie-handler-pro": "^1.0.7",
    "graphql": "catalog:api",
    "web-push": "^3.6.7"
  },
  "prettier": "@acme/prettier-config"
}